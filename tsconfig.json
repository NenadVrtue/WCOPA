{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@globals/*": ["src/components/global/*"],
      "@sanity/*": ["src/sanity/*"],
      "@ui/*": ["src/components/ui/*"],
      "@sections/*":["src/sections/*"]
    }

  }
}